           <li data-target="#da" data-slide-to="1" class=""></li>
           <li data-target="#da" data-slide-to="2" class=""></li>
           <li data-target="#da" data-slide-to="3" class=""></li>
       </ol>
       <div class="carousel-inner">
           <div class="carousel-item active"><img src="image.jpeg"></div>
           <div class="carousel-item "><img src="image1.jpg"></div>
           <div class="carousel-item "><img src="image2.jpg"></div>
           <div class="carousel-item "><img src="image3.jpg"></div>
       </div>
       <button class="carousel-control-prev" data-target="#da" data-slide="prev">back</button>
       <button class="carousel-control-next" data-target="#da" data-slide="next">next</button>
   </div>
