modal
  <button class="btn btn-primary" data-toggle="modal" data-target="#ma">show modal</button>
    <div class="modal" id="ma">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">modal header</div>
                <div class="modal-body">modelbody Lorem ipsum dolor sit amet, consectetur adipisicing elit,</div>
                <div><button class="btn btn-primary "data-dismiss="modal" style="float: right;">close</button></div>
            </div>
        </div>
    </div>
